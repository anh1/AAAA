<h1>Listing contacts</h1>

<table border=2>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Subject</th>
    <th>Action</th>
    <th>Action</th>
    <th>Action</th>
  </tr>

<% @contacts.each do |contact| %>
  <tr>
    <td><%= contact.name %></td>
    <td><%= contact.email %></td>
    <td><%= contact.subject %></td>
    <td><%= link_to 'Show', contact_path(contact) %></td>
    <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
    <td><%= link_to 'Destroy', contact, :confirm=>'Are You Sure', :method=>:delete %></td>
  </tr>
<% end %>
</table>

<br />

