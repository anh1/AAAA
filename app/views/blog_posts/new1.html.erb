<h1>New blog_post</h1>

<%=form_for :blog_post, :url => {:controller => "blog_posts", :action => "create"} do |f| %>
  <p>

  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>
  <div class="field">
    <%= f.label :Description %><br />
    <%= f.text_area :body %>
  </div>

  <p>
    <%= f.submit "Add Post" %>
  </p>
<% end %>

<%= link_to 'Back', blog_posts_path %>
